<div class="articleWrapper">
    <p>Drawing from index</p>
    {{#each articles}}
    <div class="articleContainer">
        <div class="articleImageDiv">
            <img class="articleImage" src="{{this.imageURL}}">
        </div>
        <div class="articleTitle">
            <p>{{this.headline}}</p>
        </div>
    </div>
    {{/each}}
</div>